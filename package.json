{
    "name": "mixdrinks",
    "version": "1.0.0",
    "private": false,
    "type": "module",
    "scripts": {
        "dev": "npm install && cross-env NODE_ENV=development nuxt dev",
        "build": "cross-env NODE_ENV=production nuxt build",
        "start": "cross-env NODE_ENV=production nuxt start",
        "export": "cross-env NODE_ENV=production nuxt export",
        "serve": "cross-env NODE_ENV=production nuxt serve",
        "buildstart": "cross-env NODE_ENV=production nuxt build && nuxt start",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier . --check",
        "lint:fix": "eslint . --fix && prettier --write --list-different ."
    },
    "engines": {
        "node": "20.17.0"
    },
    "dependencies": {
        "@nuxtjs/sitemap": "^6.1.4",
        "bcrypt": "^5.1.1",
        "mongodb": "6.8",
        "nuxt": "3.13.2",
        "prettier": "^3.3.3",
        "vue": "3.5.8"
    },
    "devDependencies": {
        "@nuxt/devtools": "latest",
        "@nuxt/eslint": "^0.5.7",
        "@nuxt/eslint-config": "^0.5.7",
        "cross-env": "^7.0.3",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "sass": "1.77.6",
        "sass-loader": "^16.0.2"
    }
}
